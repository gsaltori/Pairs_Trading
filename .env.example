# MetaTrader 5 Configuration for IC Markets Global
# Copy this file to .env and fill in your credentials

# MT5 Account Credentials
MT5_LOGIN=12345678
MT5_PASSWORD=your_password_here
MT5_SERVER=ICMarketsSC-Demo

# For live trading, use:
# MT5_SERVER=ICMarketsSC-Live

# MT5 Terminal Path (optional, auto-detected if not specified)
# MT5_TERMINAL_PATH=C:\Program Files\IC Markets Global - MetaTrader 5\terminal64.exe

# Connection Settings
MT5_TIMEOUT=60000

# Trading Settings
MT5_MAGIC_NUMBER=123456
MT5_DEVIATION=20

# Symbol Suffix (if your broker uses suffixes like ".a" or "m")
MT5_SYMBOL_SUFFIX=
