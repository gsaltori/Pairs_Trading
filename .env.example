# OANDA API Credentials
# Copy this file to .env and fill in your credentials

# Your OANDA account ID (found in account settings)
OANDA_ACCOUNT_ID=xxx-xxx-xxxxxxx-xxx

# Your OANDA API key (generate in account settings)
OANDA_API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Environment: 'practice' for demo account, 'live' for real trading
OANDA_ENVIRONMENT=practice
